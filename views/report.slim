                span.closebtn2 onclick="URLRemove2('skup','def_skup[]')" x
            - if @dict_info[@dictcode]['type'] == 'sign'
              - if @params['schvcelni'] 
                - if @params['schvcelni'] == 'ano'
                  a.btn class="btn-secondary report-ano" onclick="URLChange('schvcelni','ne')" #{I18n.t("admin.schvcelni")}
                - if @params['schvcelni'] == 'ne'
                  a.btn class="btn-secondary report-ne" onclick="URLChange('schvcelni','ano')" #{I18n.t("admin.not")}#{I18n.t("admin.schvcelni")}
                span.closebtn2 onclick="URLRemove('schvcelni')" x               
              - if @params['schvbocni'] 
                - if @params['schvbocni'] == 'ano'
                  a.btn class="btn-secondary report-ano" onclick="URLChange('schvbocni','ne')" #{I18n.t("admin.schvbocni")}
                - if @params['schvbocni'] == 'ne'
                  a.btn class="btn-secondary report-ne" onclick="URLChange('schvbocni','ano')" #{I18n.t("admin.not")}#{I18n.t("admin.schvbocni")}
                span.closebtn2 onclick="URLRemove('schvbocni')" x
                
              - if @params['bez_sw'] 
                - if @params['bez_sw'] == 'ano'
                  a.btn class="btn-secondary report-ano" onclick="URLChange('bez_sw','ne')" #{I18n.t("admin.sw")}
                - if @params['bez_sw'] == 'ne'
                  a.btn class="btn-secondary report-ne" onclick="URLChange('bez_sw','ano')" #{I18n.t("admin.not")}#{I18n.t("admin.sw")}
                span.closebtn2 onclick="URLRemove('bez_sw')" x

              - if @params['nes_sw'] 
                - if @params['nes_sw'] == 'ano'
                  a.btn class="btn-secondary report-ano" onclick="URLChange('nes_sw','ne')" #{I18n.t("admin.schvsw")}
                - if @params['nes_sw'] == 'ne'
                  a.btn class="btn-secondary report-ne" onclick="URLChange('nes_sw','ano')" #{I18n.t("admin.not")}#{I18n.t("admin.schvsw")}
                span.closebtn2 onclick="URLRemove('nes_sw')" x

          p
            | #{I18n.t("admin.desc")}:
            - if @dict_info[@dictcode]['type'] == 'sign'
              - if @params['mkok'] 
                - if @params['mkok'] == 'ano'
                  a.btn class="btn-secondary report-ano" onclick="URLChange('mkok','ne')" #{I18n.t("admin.mkok")}
                - if @params['vyznamvid'] == 'ne'
                  a.btn class="btn-secondary report-ne" onclick="URLChange('mkok','ano')" #{I18n.t("admin.not")}#{I18n.t("admin.mkok")}
                span.closebtn2 onclick="URLRemove('mkok')" x

            - if @params['sldruh'] == 'ne' and @params['slovni_druh'] != ''
