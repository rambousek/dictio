                - unless @params['skup'] and @params['def_skup[]'] == 'obecna'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','obecna','skup','ne')" #{I18n.t("admin.group.obecna")}
                - unless @params['skup'] and @params['def_skup[]'] == 'upol'
