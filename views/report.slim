                - unless @params['skup'] and @params['def_skup[]'] == 'biologie'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','biologie','skup','ne')" #{I18n.t("admin.group.biologie")}  
                - unless @params['skup'] and @params['def_skup[]'] == 'informatika'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','informatika','skup','ne')" #{I18n.t("admin.group.informatika")}
                - unless @params['skup'] and @params['def_skup[]'] == 'matematika'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','matematika','skup','ne')" #{I18n.t("admin.group.matematika")}
                - unless @params['skup'] and @params['def_skup[]'] == 'vut_me'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','vut_me','skup','ne')" #{I18n.t("admin.group.vut_me")}                            
                - unless @params['skup'] and @params['def_skup[]'] == 'podnikani'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','podnikani','skup','ne')" #{I18n.t("admin.group.podnikani")}
                - unless @params['skup'] and @params['def_skup[]'] == 'spec'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','spec','skup','ne')" #{I18n.t("admin.group.spec")}
                - unless @params['skup'] and @params['def_skup[]'] == 'aka'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','aka','skup','ne')" #{I18n.t("admin.group.aka")}
                - unless @params['skup'] and @params['def_skup[]'] == 'krest'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','krest','skup','ne')" #{I18n.t("admin.group.krest")}
                - unless @params['skup'] and @params['def_skup[]'] == 'podnikani'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','podnikani','skup','ne')" #{I18n.t("admin.group.podnikani")}
                - unless @params['skup'] and @params['def_skup[]'] == 'test'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','test','skup','ne')" #{I18n.t("admin.group.test")}
                - unless @params['skup'] and @params['def_skup[]'] == 'mdd'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','mdd','skup','ne')" #{I18n.t("admin.group.mdd")}
                - unless @params['skup'] and @params['def_skup[]'] == 'ls'
                  a.btn class="btn-secondary report-" onclick="URLChange2('def_skup[]','ls','skup','ne')" #{I18n.t("admin.group.ls")}
            div
              | #{I18n.t("admin.lemma")}:                                        
              div
                - unless @params['typhesla'] and @params['seltyphesla'] == 'single'
                  a.btn class="btn-secondary report-" onclick="URLChange2('seltyphesla','single','typhesla','ne')" #{I18n.t("admin.single")}            
                - unless @params['typhesla'] and @params['seltyphesla'] == 'derivat'
                  a.btn class="btn-secondary report-" onclick="URLChange2('seltyphesla','derivat','typhesla','ne')" #{I18n.t("admin.derivat")}
                - unless @params['typhesla'] and @params['seltyphesla'] == 'kompozitum'
                  a.btn class="btn-secondary report-" onclick="URLChange2('seltyphesla','kompozitum','typhesla','ne')" #{I18n.t("admin.kompozitum")}
                - if @dict_info[@dictcode]['type'] == 'sign'
                  - unless @params['typhesla'] and @params['seltyphesla'] == 'fingerspell'
                    a.btn class="btn-secondary report-" onclick="URLChange2('seltyphesla','fingerspell','typhesla','ne')" #{I18n.t("admin.fingerspell")}              
                - unless @params['typhesla'] and @params['seltyphesla'] == 'collocation'
                  a.btn class="btn-secondary report-" onclick="URLChange2('seltyphesla','collocation','typhesla','ne')" #{I18n.t("admin.collocation")}              

              - if @dict_info[@dictcode]['type'] == 'sign'
                div            
                  - unless @params['schvcelni']
                    a.btn class="btn-secondary report-" onclick="URLAppend('schvcelni')" #{I18n.t("admin.schvcelni")}
                  - unless @params['schvbocni']
                    a.btn class="btn-secondary report-" onclick="URLAppend('schvbocni')" #{I18n.t("admin.schvbocni")}
                  - unless @params['bez_sw']
                    a.btn class="btn-secondary report-" onclick="URLAppend('bez_sw')" #{I18n.t("admin.sw")}
                  - unless @params['nes_sw']
                    a.btn class="btn-secondary report-" onclick="URLAppend('nes_sw')" #{I18n.t("admin.schvsw")}
            - if @dict_info[@dictcode]['type'] == 'sign'
              div
                | #{I18n.t("admin.desc")}:
                - unless @params['mkok']
                  a.btn class="btn-secondary report-" onclick="URLAppend('mkok')" #{I18n.t("admin.mkok")}
            div
              span onclick="showhide('sldruh')" style="cursor: pointer"
                | #{I18n.t("entry.lexical")}: >>
              div id="sldruh" style="display: none"
                - unless @params['sldruh'] and @params['slovni_druh'] == 'subst'
                  a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','subst','sldruh','ne')" #{I18n.t("gram.slovni_druh.subst")}
                - unless @params['sldruh'] and @params['slovni_druh'] == 'adj'
