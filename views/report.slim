            div
              span onclick="showhide('comm')" style="cursor: pointer"
                | #{I18n.t("admin.comments")}: >>
              div id="comm" style="display: none" 
                - unless @params['koment'] and @params['komentbox'] == 'boxlemma'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','boxlemma','koment','ne')" #{I18n.t("admin.koment.boxlemma")}      
                - unless @params['koment'] and @params['komentbox'] == 'boxcolloc'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','boxcolloc','koment','ne')" #{I18n.t("admin.koment.boxcolloc")}
                - if @dict_info[@dictcode]['type'] == 'sign'
                  - unless @params['koment'] and @params['komentbox'] == 'video'
                    a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','video','koment','ne')" #{I18n.t("admin.koment.video")}
                  - unless @params['koment'] and @params['komentbox'] == 'swfield'
                    a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','swfield','koment','ne')" #{I18n.t("admin.koment.swfield")}
                  - unless @params['koment'] and @params['komentbox'] == 'hamnbox'
                    a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','hamnbox','koment','ne')" #{I18n.t("admin.koment.hamnbox")}
                - unless @params['koment'] and @params['komentbox'] == 'gramdesc'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','gramdesc','koment','ne')" #{I18n.t("admin.koment.gramdesc")}
                - unless @params['koment'] and @params['komentbox'] == 'styldesc'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','styldesc','koment','ne')" #{I18n.t("admin.koment.styldesc")}
                - unless @params['koment'] and @params['komentbox'] == 'vyznam'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','vyznam','koment','ne')" #{I18n.t("admin.koment.vyznam")}
                - unless @params['koment'] and @params['komentbox'] == '_us'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','_us','koment','ne')" #{I18n.t("admin.koment._us")}
                - unless @params['koment'] and @params['komentbox'] == 'vazby'
                  a.btn class="btn-secondary report-" onclick="URLChange2('komentbox','vazby','koment','ne')" #{I18n.t("admin.koment.vazby")}
      - if @duplicate['duplicate'].select{|info| info['code'] == @dictcode}.size > 0
        div style="color: red; font-style: italic; font-variant: all-small-caps"
          | #{I18n.t("admin.dups_warn")} (#{@duplicate['duplicate'].select{|info| info['code'] == @dictcode}[0]['count']}). 
          a href="/#{@dictcode}/duplicate" #{I18n.t("admin.show")} 

      table.table.table--no-margin.report-table data-resultcount=@report['resultcount']
        tr
          th width="10%" (#{@report['resultcount']})
          th
          - if @params['komentbox']
            th
              - if @params['koment'] == 'ne'
                | Komentáře v poli: 
              - if @params['koment'] == 'ano'
                | Bez komentářů v poli  
              - if @params['komentbox'] == ''
                | neurčeno
              - if @params['komentbox'] == 'boxlemma'
                | technické info
              - if @params['komentbox'] == 'boxcolloc'
                | typ hesla
              - if @params['komentbox'] == 'video'
                | záhlaví (video)
              - if @params['komentbox'] == 'swfieldset'
                | SW
              - if @params['komentbox'] == 'hamnbox'
                | HNS 
              - if @params['komentbox'] == 'gramdesc'
                | gramatika
              - if @params['komentbox'] == 'styldesc'
                | stylistika
              - if @params['komentbox'] == 'vyznam'
                | význam
              - if @params['komentbox'] == '_us'
                | příklady užití
              - if @params['komentbox'] == 'vazby'
                | ekvivalenty
          - if @is_admin or @is_edit
            - if @params.count{|k,_| k =~ /sw/} > 0
              th
            th width="10%"        
        == slim :reportresultlist
      - if @report['resultcount'].to_i > @report['entries'].size
        a.btn.load_next_report data-dict=@dictcode #{I18n.t("search.load_next")}
