              | #{I18n.t("admin.sem")}:  
              - if @dict_info[@dictcode]['type'] == 'sign'
                - unless @params['vyznamvid']
                  a.btn class="btn-secondary report-" onclick="URLAppend('vyznamvid')" #{I18n.t("admin.vyznamvid")}
                - unless @params['vyznam']
                  a.btn class="btn-secondary report-" onclick="URLAppend('vyznam')" #{I18n.t("admin.vyznam")}    
                - unless @params['usagevid']
                  a.btn class="btn-secondary report-" onclick="URLAppend('usagevid')" #{I18n.t("admin.usagevid")}
                - unless @params['usage']
                  a.btn class="btn-secondary report-" onclick="URLAppend('usage')" #{I18n.t("admin.usage")}

